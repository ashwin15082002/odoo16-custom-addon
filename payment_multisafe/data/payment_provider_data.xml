<?xml version="1.0" encoding="utf-8"?>

<odoo noupdate="1">

    <record id="payment_provider_multisafe_pay"
            model="payment.provider">

        <field name="name">Multisafe</field>
        <field name="code">multisafe</field>
        <field name="image_128" type="base64"
               file="payment_multisafe/static/img/logo.png"/>

    </record>
    <record id="account_payment_method_multisafe"
            model="account.payment.method">
        <field name="name">Multisafe</field>
        <field name="code">multisafe</field>
        <field name="payment_type">inbound</field>
    </record>

</odoo>
